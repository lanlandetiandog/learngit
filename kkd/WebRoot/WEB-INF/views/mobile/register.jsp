<%@ page contentType="text/html;charset=UTF-8"%><%@ include file="/static/common/taglib.jsp"%><!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><meta http-equiv="Content-Type" content="text/html; charset=utf-8" /><meta name="viewport"    content="width=device-width,initial-scale=1,user-scalable=no" />    <%@ include file="/static/common/meta.jsp"%><script type="text/javascript" src="${ctx}/static/plugin/js/timer.jquery.js"></script><script type="text/javascript" src="${ctx}/static/kingkaid/js/mobile/register.js"></script><style type="text/css">body {    margin: 0;    padding: 0;    font-size: 14px;    font-family: "微软雅黑";}.page_header {    width: 100%;}.page_content {    width: 90%;    overflow: hidden;    margin-top: 30px;    margin: 5%;    background-color: #fff;}.m-login_title {    color: #ea6660;    font-size: 20px;    font-weight: bold;    border-bottom: 3px solid #ea6660;}.m-register_label {    float: left;    margin-top: 0px;    color: #4d4d4d;}.m-register_label label {    display: block;    margin: 30px 0 38px 0;    font-size: 16px;}.m-register_area {    float: left;    margin-top: 20px;    width: 100%;}.m-catch_code {    background-color: #5d89c9;    padding: 0 5px;    color: #fff;    border-radius: 4px;    height: 29px;    line-height: 29px;    display: inline-block;    font-size: 12px;    cursor: pointer;}.m-catch_code_gray {    background-color: #99a7ba;    padding: 0 5px;    color: #fff;    border-radius: 4px;    height: 29px;    line-height: 29px;    display: inline-block;    font-size: 12px;    cursor: default;}.register-input {    width: 70%;    height: 30px;    line-height: 30px;    border: 1px solid #acacac;    border-radius: 4px;    padding-left: 8px;    font-size: 14px;}.error-msg {    color: #E7222B;    font-size: 12px;}.login-bottom {    margin-top: 20px;    text-align: right;}.login-bottom a {    color: #000;    text-decoration: underline;    margin-left: 10px;}.login-bottom a:hover {    color: #5d89c9;}.login_btn {    width: 100%;    border-radius: 4px;    background-color: #5d89c9;    height: 42px;    line-height: 42px;    text-align: center;    font-size: 16px;    color: #fff;    cursor: pointer;}.blue_link {    color: #5c88c9;    text-decoration: underline;}.input-unit {    height: 59px;}</style></head><body>    <div class="page_header">        <div style="margin: 30px 0 30px 5%;">            <a href="${ctx}/"><img src="${ctx}/static/kingkaid/images/logo.jpg" /></a>        </div>                <div>            <img style="width: 100%; height: 182px;" src="${ctx}/static/kingkaid/images/top.jpg" />        </div>    </div>    <div class="page_content">        <div class="m-register_label">            <div class="m-login_title">注册</div>        </div>                <form id="form_register" class="m-register_area" action="${ctx}/member/register" method="post">            <c:if test="${not empty errorMsg}">            <div id="errorMsg" style="margin-top: 10px; margin-bottom: 10px; color: #E7222B; font-size: 12px; height:18px; width:70%; background-color: #f2dddd; border-color: #eed5c6; border-radius: 3px">${errorMsg}</div>            </c:if>            <div class="input-unit">                <input type="text" id="filter_membername" name="filter_membername" tabindex="1" class="register-input" placeholder="用户名" />                                <div></div>            </div>            <div class="input-unit">                <input type="password" id="filter_loginpassword" name="filter_loginpassword" tabindex="2" class="register-input" placeholder="登录密码" />                                <div></div>            </div>            <div class="input-unit">                <input type="password" id="dpassword" name="dpassword" tabindex="3" class="register-input" placeholder="重复密码" />                <div></div>            </div>            <div class="input-unit">                <input type="text" id="filter_mobilenumber" name="filter_mobilenumber" tabindex="4" class="register-input" placeholder="手机号码" />                <div></div>            </div>            <div class="input-unit">                <input type="text" id="filter_verifycode" name="filter_verifycode" tabindex="5" class="register-input" placeholder="验证码" />                <button type="button" id="fetch-vcode" class="m-catch_code">获取验证码</button>                <div></div>            </div>            <div class="input-unit">                <input type="checkbox" id="if_invitecode_chkbox" name="if_invitecode_chkbox" tabindex="6" checked="checked" <c:if test="${not empty inviteCode}">readonly="true"</c:if>/>                 <input type="text" id="filter_invitecode" name="filter_invitecode" class="register-input" tabindex="7" style="width: 61%;" placeholder="聚财码" <c:if test="${not empty inviteCode}">value='${inviteCode}' disabled</c:if> />                <div></div>            </div>            <div style="margin-bottom: 15px;">                <input type="checkbox" id="if_read_chkbox" name="if_read_chkbox" tabindex="8"/>                 <span>我同意</span><a class="blue_link" href="protocol.html" target="_blank">金开贷会员服务协议</a>                <div></div>                            </div>            <input type="submit" id="btn_register" class="login_btn" style="width: 100%; margin-top: 10px; font-family: '微软雅黑','宋体',STHeiti,Verdana,Arial,Helvetica,sans-serif;" tabindex="9" value="注册" />            <div class="login-bottom" style="margin-top: 10px;">            </div>        </form>    </div></body></html>